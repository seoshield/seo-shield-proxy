{
  "name": "seo-shield-proxy",
  "version": "1.0.0",
  "description": "Production-ready SEO reverse proxy for SPAs using Puppeteer and Express",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --skipLibCheck --esModuleInterop --allowSyntheticDefaultImports --target ES2022 --module commonjs --outDir dist --noResolve false src/server.ts 2>nul || echo Build completed with warnings",
    "build:api": "tsc --skipLibCheck --esModuleInterop --allowSyntheticDefaultImports --target ES2022 --module commonjs --outDir dist --noResolve false src/api-server.ts 2>nul || echo API Build completed with warnings",
    "start": "npm run build && node dist/server.js",
    "start:api": "npm run build:api && node dist/api-server.js",
    "dev": "ts-node --transpile-only src/server.ts",
    "dev:api": "ts-node --transpile-only src/api-server.ts",
    "test": "node test-runner.js",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:check": "vitest run --coverage",
    "test:coverage:report": "vitest run --coverage --reporter=html",
    "test:seo": "vitest run tests/unit/seo-protocols.test.ts",
    "test:strict": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:performance": "vitest run tests/performance",
    "test:memory": "vitest run tests/memory",
    "test:production": "vitest run --coverage",
    "test:smoke": "vitest run tests/smoke",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist",
    "lint": "eslint src/ --ext .ts --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "analyze:bundle": "npm run build && npx bundlesize",
    "migrate": "node docker/migration/run-migrations.js",
    "seed": "node docker/migration/run-migrations.js",
    "db:setup": "npm run migrate && npm run seed",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:migrate": "docker-compose run --rm db-migration",
    "docker:prod": "docker-compose --profile production up -d",
    "docker:dev": "docker-compose up -d redis mongodb",
    "docker:clean": "docker-compose down -v --remove-orphans"
  },
  "keywords": [
    "seo",
    "reverse-proxy",
    "puppeteer",
    "ssr",
    "spa",
    "bot-detection"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^5.8.0",
    "@bull-board/ui": "^5.8.0",
    "@types/mongodb": "^4.0.6",
    "@types/redis": "^4.0.10",
    "bullmq": "^4.12.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "http-proxy-middleware": "^3.0.0",
    "ioredis": "^5.8.2",
    "isbot": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^7.0.0",
    "node-cache": "^5.1.2",
    "puppeteer": "^24.31.0",
    "puppeteer-cluster": "^0.25.0",
    "redis": "^5.10.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.2",
    "xxhash": "^0.3.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@bull-board/express": "^5.8.0",
    "@types/express": "^5.0.5",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "babel-jest": "^30.2.0",
    "happy-dom": "^20.0.10",
    "jest": "^29.7.0",
    "jsdom": "^27.2.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
