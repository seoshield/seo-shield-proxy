# Test Environment Variables
NODE_ENV=test
PORT=8080
TARGET_URL=http://localhost:3000
CACHE_TTL=60
CACHE_TYPE=memory
REDIS_URL=redis://localhost:6379
PUPPETEER_TIMEOUT=10000
MAX_CONCURRENT_RENDERS=2
NO_CACHE_PATTERNS=/api/|/admin/
CACHE_PATTERNS=/blog/|/docs/
CACHE_BY_DEFAULT=true
CACHE_META_TAG=x-seo-shield-cache
ADMIN_PASSWORD=test123
USER_AGENT=Mozilla/5.0 (compatible; SEOShieldProxy/1.0; +https://github.com/seoshield/seo-shield-proxy)

# SEO Protocols Configuration
SEO_PROTOCOLS_ENABLED=true
SEO_CONTENT_HEALTH_CHECK=true
SEO_VIRTUAL_SCROLL=true
SEO_ETAG_STRATEGY=true
SEO_CLUSTER_MODE=false
SEO_SHADOW_DOM=true
SEO_CIRCUIT_BREAKER=true

# Test-specific settings
CIRCUIT_BREAKER_ERROR_THRESHOLD=50
VIRTUAL_SCROLL_STEPS=3
CONTENT_HEALTH_MIN_BODY_LENGTH=100